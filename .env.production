# Database
MONGODB_URL=mongodb://mongodb:27017
DATABASE_NAME=quantum_dashboard_prod
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=secure_mongo_password_change_me

# Redis
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=secure_redis_password_change_me

# JWT
JWT_SECRET_KEY=ultra_secure_jwt_secret_key_change_in_production_minimum_32_chars
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60

# CORS
CORS_ORIGINS=["https://yourdomain.com","https://www.yourdomain.com"]
ALLOWED_HOSTS=["yourdomain.com","www.yourdomain.com","app"]

# File Upload
MAX_FILE_SIZE=52428800
UPLOAD_FOLDER=uploads

# Rate Limiting
RATE_LIMIT_PER_MINUTE=120

# SSL/Security
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem
